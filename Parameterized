class B{
    int a,b,c;
    B(int a,int b,int c){
        this.a = a;
        this.b = b;
        this.c = c ;
        System.out.println(" The values are :" + a + " " + b + " " + c);

    }
}

public class Parameterized {
    public static void main(String[] args) {
        B  obj = new B(10,20,30);


    }
}
