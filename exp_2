package PrimeTest;

public class PrimeTest {
    static boolean isPrime(int n) {
        if (n < 2) return false;
        for (int i = 2; i <= Math.sqrt(n); i++)
            if (n % i == 0) return false;
        return true;
    }

    public static void main(String[] args) {
        int[] testCases = {2, 15, 17, 1, 0};
        for (int n : testCases)
            System.out.println("Input " + n + " â†’ Output: " + (isPrime(n) ? "Prime" : "Not Prime"));
    }
}
